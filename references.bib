@ARTICLE{Stacey2019-je,
  title    = "{ProGeM}: a framework for the prioritization of candidate causal
              genes at molecular quantitative trait loci",
  author   = "Stacey, David and Fauman, Eric B and Ziemek, Daniel and Sun,
              Benjamin B and Harshfield, Eric L and Wood, Angela M and
              Butterworth, Adam S and Suhre, Karsten and Paul, Dirk S",
  abstract = "Quantitative trait locus (QTL) mapping of molecular phenotypes
