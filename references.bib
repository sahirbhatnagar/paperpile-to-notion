  file     = "All Papers/MR/Gkatzionis et al. 2023 - Statistical methods for cis-Mendelian randomization with two-sample summary-level data.pdf",
  keywords = "Bayesian variable selection; Mendelian randomization; correlated
              instruments; factor analysis; principal components analysis;
              pruning;MR",
  language = "en"
}

