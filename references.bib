  keywords = "Asymptotics; Family-wise error rate; Multiple hypotheses; Scan
              statistics; Signal detection; Whole genome sequencing association
              studies;Stats - Region Based Tests;RVAT",
  language = "en"
}

@ARTICLE{Mukhopadhyay2010-ia,
